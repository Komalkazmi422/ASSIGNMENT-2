import java.io.FileWriter;
import java.io.IOException;
public class OrderService {
    private final Logger logger;
    private PaymentType paymentType;
    private UserType userType;
    public OrderService(Logger logger, PaymentType paymentType, UserType userType) {
        this.logger = logger;
        this.paymentType = paymentType;
        this.userType = userType;
    }
    public void processOrder(Order order) throws IOException {
        logger.logStart(order.getUserName());

        if (order.isUrgent()) {
            order.setQuantity(order.getQuantity() + 1);
        }
        if (order.getQuantity() <= 0 || order.getPrice() <= 0) {
            System.out.println("Invalid quantity or price");
            return;
        }
        double total = calculateTotal(order);
        try (FileWriter fw = new FileWriter("order_data.txt", true)) {
            fw.write("U:" + order.getUserName() + " P:" + order.getProductId() + " T:" + total + "\n");
        }
        printReport(order, total);
        logger.logEnd(order.getUserName());
    }
    private double calculateTotal(Order order) {
        double baseA = 10;
        double baseB = 20;
        double total = order.getQuantity() * order.getPrice() + baseA + baseB;
        switch (userType) {
            case REGULAR -> total -= 5;
            case SILVER -> total -= 2;
            case GOLD -> total += 5;
        }
        return total;
    }
    private void printReport(Order order, double total) {
        System.out.println("----- ORDER REPORT -----");
        System.out.println("User: " + order.getUserName());
        System.out.println("Product: " + order.getProductId());
        System.out.println("Quantity: " + order.getQuantity());
        System.out.println("Total: " + total);
        System.out.println("Date: " + order.getDate());
        System.out.println("Payment Type: " + paymentType);
    }
}
