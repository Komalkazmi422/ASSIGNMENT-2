import java.io.File;
import java.util.Scanner;
public class SettingsManager {
    private UserType userType = UserType.REGULAR;
    private PaymentType paymentType = PaymentType.CASH;
    public void loadSettings(String filePath) {
        try {
            File file = new File(filePath);
            Scanner s = new Scanner(file);
            while (s.hasNextLine()) {
                String line = s.nextLine();
                if (line.startsWith("U")) userType = switch (Integer.parseInt(line.substring(2))) {
                    case 1 -> UserType.REGULAR;
                    case 2 -> UserType.SILVER;
                    case 3 -> UserType.GOLD;
                    default -> UserType.REGULAR;
                };
                if (line.startsWith("P")) paymentType = switch (line.substring(2)) {
                    case "CASH" -> PaymentType.CASH;
                    case "CARD" -> PaymentType.CARD;
                    case "BANK" -> PaymentType.BANK;
                    default -> PaymentType.OTHER;
                };
            }
        } catch (Exception ex) {
            System.out.println("Settings not loaded");
        }
    }
    public UserType getUserType() { return userType; }
    public PaymentType getPaymentType() { return paymentType; }
}
